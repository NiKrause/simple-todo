{
	"name": "simple-todo",
	"private": true,
	"version": "0.1.14",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "pnpm run test:e2e",
		"test:ui": "playwright test --ui",
		"test:e2e": "playwright test",
		"test:p2p": "playwright test e2e/p2p-todo.spec.js",
		"test:webrtc": "playwright test e2e/webrtc-test.spec.js",
		"test:webrtc:local": "playwright test e2e/webrtc-test.spec.js --project=chromium",
		"test:webrtc:browserstack": "BROWSERSTACK_BUILD_NAME=webrtc-test-$(date +%s) playwright test e2e/webrtc-test.spec.js"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"dotenv": "^17.2.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"node-fetch": "^3.3.2",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"wait-on": "^8.0.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"overrides": {
			"ms": "2.1.3"
		}
	},
	"dependencies": {
		"@chainsafe/libp2p-gossipsub": "^14.1.1",
		"@chainsafe/libp2p-noise": "^16.1.4",
		"@chainsafe/libp2p-yamux": "^7.0.4",
		"@ipld/dag-cbor": "^9.2.1",
		"@libp2p/autonat": "^2.0.37",
		"@libp2p/bootstrap": "^11.0.46",
		"@libp2p/circuit-relay-v2": "^3.2.23",
		"@libp2p/crypto": "^5.1.7",
		"@libp2p/dcutr": "^2.0.37",
		"@libp2p/identify": "^3.0.38",
		"@libp2p/pubsub-peer-discovery": "^11.0.2",
		"@libp2p/webrtc": "^5.2.23",
		"@libp2p/websockets": "^9.2.18",
		"@orbitdb/core": "^3.0.2",
		"buffer": "^6.0.3",
		"helia": "^5.5.0",
		"libp2p": "^2.9.0",
		"lucide-svelte": "^0.539.0",
		"multiformats": "^13.3.4",
		"uint8arrays": "^5.1.0",
		"vite-plugin-node-polyfills": "^0.24.0"
	}
}
